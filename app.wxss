/**app.wxss**/
.container {
  height: 100%;
  width: 100%;
  /* display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between; */
  overflow:hidden;
  background: #fff;
} 
.title_home{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    font-size: 34rpx;
    color: #333333;
    border-bottom: 1rpx solid #DFDFDF;
    height:65px;
    line-height:85px;
}

/* 返回按钮 */
.come_back{
    position: absolute;
    top: 0rpx;
    line-height: 85px;
    width: 50rpx;
    height: 85px;
    left: 25rpx;
    /* transform: rotate(180deg);  */
    z-index: 999;
}
.come_back image{
    width:18rpx;
    height:32rpx;
    
}


/* 弹窗样式 */
.pop-ups-bg{
    width: 100%;
    height: 100vh;
    position: absolute;
    bottom: 0rpx;
    left: 0rpx;
    background: rgba(0, 0, 0, 0.5);
    z-index: 99999;
}
.pop-ups{
    width: 630rpx;
    height: 370rpx;
    border-radius: 30rpx;
    background: #fff;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -185rpx;
    margin-left: -315rpx;
}
.modal-title {
  padding-top: 50rpx;
  font-size: 36rpx;
  color: #333333;
  text-align: center;
}
.modal-title-vice{
    text-align: center;
    font-size: 32rpx;
    color: #333333;
    padding-top: 35rpx;
}
.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  font-size: 34rpx;
  line-height: 86rpx;
  padding-top: 50rpx;
}
.btn-cancel {
  width: 260rpx;
  text-align: center;
  background: #ccc;
  margin-left: 40rpx;
  border-radius: 10rpx;
  background: #E9E9E9;
  color: #999999
}
.btn-confirm {
  width: 260rpx;
  text-align: center;
  background: #ccc;
  margin-left: 30rpx;
  border-radius: 10rpx;
  background: #EC82F5;
  color: #333333;
}


.mask{
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  z-index: 99990;
  background: rgba(0, 0, 0, 0.7)
}
.pop_ups{
  width: 570rpx;
  height: 376rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -188rpx;
  margin-left: -285rpx;
  background: #fff;
  border-radius: 10rpx;
}
.pop_top{
  width: 100%;
  height: 92rpx;
  line-height: 92rpx;
  border-bottom: 1rpx solid#EDEDED;
  text-align: center;
  font-size: 32rpx;
}
.pop_text{
  width: 254rpx;
  font-size: 28rpx;
  color: #666666;
  margin: auto;
  margin-top: 50rpx;
  word-wrap:break-word;
  white-space : normal;
  text-align: center;
}
.pop_but_bot{
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: absolute;
  bottom: 12rpx;
}
.pop_but{
  width: 260rpx;
  height: 74rpx;
  text-align: center;
  line-height: 74rpx;
  border-radius: 6rpx;
  margin-left: 14rpx;
  margin-right: 4rpx;
  font-size: 32rpx;
}
.pop_quxiao{
  border: 1rpx solid#D7D7D7;
}
.pop_succ{
  background: #F9B745;
  color: #fff;
}